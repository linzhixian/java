<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.funugame.parkour.dao.UserAchievementMapper">
	<cache eviction="LRU" flushInterval="86400000" size="2048" />



	<insert id="insert" parameterType="UserAchievement">
		INSERT INTO
		user_achievement(uid,id,status)
		value(#{uid},#{id},#{status});

	</insert>
	<select id="selectList" parameterType="int" resultType="UserAchievement">
		select *
		from user_achievement where uid=#{uid}
	</select>


	<update id="update" parameterType="UserAchievement">
		UPDATE user_achievement set status=#{status}
		WHERE uid = #{uid} and id=#{id}
	</update>


</mapper>