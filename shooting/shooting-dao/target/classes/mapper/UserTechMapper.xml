<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.funugame.parkour.dao.UserTechMapper">
	<cache eviction="LRU" flushInterval="86400000" size="2048" />



	<insert id="insert" parameterType="UserTech">
		INSERT INTO
		user_tech(uid,id,level)
		value(#{uid},#{id},#{level});

	</insert>
	<select id="selectList" parameterType="int" resultType="UserTech">
		select *
		from user_tech where uid=#{uid}
	</select>





</mapper>